{
    "scripts": {
        "start": "nodemon -w webpack.config.js -w webpack.parts.js --exec webpack-dev-server --mode development --env development --open --hot",
        "build": "webpack --mode production --env production",
        "test": "jest --coverage",
        "test:watch": "jest --watchAll"
    },
    "jest": {
        "testEnvironment": "jest-environment-node",
        "collectCoverageFrom": [
            "src/js/*",
            "!src/js/index.js"
        ],
        "coverageThreshold": {
            "global": {
                "statements": 50,
                "branches": 50,
                "functions": 50,
                "lines": 100
            }
        }
    },
    "name": "securities-app",
    "version": "1.0.0",
    "main": "index.js",
    "license": "MIT",
    "devDependencies": {
        "autoprefixer": "^9.1.0",
        "babel-core": "^6.26.3",
        "babel-jest": "^23.4.2",
        "babel-loader": "^7.1.5",
        "babel-preset-env": "^1.7.0",
        "babel-preset-react": "^6.24.1",
        "babel-preset-stage-2": "^6.24.1",
        "clean-webpack-plugin": "^0.1.19",
        "css-loader": "^1.0.0",
        "file-loader": "^1.1.11",
        "html-webpack-plugin": "^3.2.0",
        "jest": "^23.4.2",
        "mini-css-extract-plugin": "^0.4.1",
        "node-sass": "^4.9.2",
        "nodemon": "^1.18.3",
        "postcss-loader": "^2.1.6",
        "react-test-renderer": "^16.4.2",
